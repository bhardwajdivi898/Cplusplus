#include <iostream>
using namespace std;

int main(){
  int n , num,sum=0;
  cout<<"how many number you want to enter";
  cin>>n;
  for (int i=1;i<=n;i++){
      cout<<"enter the number";
      cin>>num;
      sum+=num;
   }  
 int Average=sum/n;
 cout<<"average of the "<<n<<"numbers is "<<Average;
return 0; 
}
